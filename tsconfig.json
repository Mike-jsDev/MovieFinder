{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src/",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@images/*": ["images/*"],
      "@theme/*": ["theme/*"],
      "@interfaces/*": ["interfaces/*"],
      "@businessLogic/*": ["businessLogic/*"],
      "@redux/*": ["redux/*"],
      "@constants/*": ["constants/*"],
      "@utils/*": ["utils/*"],
      "@hooks/*": ["hooks/*"],
      "@validations/*": ["validations/*"],
    }
  },
  "include": ["./src/**/*", "d.ts"]
}
